 000A           ; /////////////////////////////////////////////////////////////////////////
 000A           ; ////                          graphics.c                             ////
 000A           ; ////                                                                 ////
 000A           ; ////   This file contains functions to draw lines, rectangles, bars, ////
 000A           ; ////   circles and text to a display. A function which draws a       ////
 000A           ; ////   single pixel must be defined before calling the functions in  ////
 000A           ; ////   this file. Call it glcd_pixel(x, y, color) where x is the     ////
 000A           ; ////   horizontal coordinate, y is the vertical coordinate, and      ////
 000A           ; ////   color is 1 bit to turn the pixel on or off.                   ////
 000A           ; ////                                                                 ////
 000A           ; ////   * Note: (0, 0) is treated as the upper left corner            ////
 000A           ; ////                                                                 ////
 000A           ; /////////////////////////////////////////////////////////////////////////
 000A           ; ////                                                                 ////
 000A           ; ////  glcd_line(x1, y1, x2, y2, color)                               ////
 000A           ; ////     * Draws a line from the first point to the second point     ////
 000A           ; ////       with the given color                                      ////
 000A           ; ////       - color can be ON or OFF                                  ////
 000A           ; ////                                                                 ////
 000A           ; ////  glcd_rect(x1, y1, x2, y2, fill, color)                         ////
 000A           ; ////     * Draws a rectangle with one corner at point (x1,y1) and    ////
 000A           ; ////       the other corner at point (x2,y2)                         ////
 000A           ; ////       - fill can be YES or NO                                   ////
 000A           ; ////       - color can be ON or OFF                                  ////
 000A           ; ////                                                                 ////
 000A           ; ////  glcd_bar(x1, y1, x2, y2, width, color)                         ////
 000A           ; ////     * Draws a bar (wide line) from the first point to the       ////
 000A           ; ////       second point                                              ////
 000A           ; ////       - width is the number of pixels wide                      ////
 000A           ; ////       - color is ON or OFF                                      ////
 000A           ; ////                                                                 ////
 000A           ; ////  glcd_circle(x, y, radius, fill, color)                         ////
 000A           ; ////     * Draws a circle with center at (x,y)                       ////
 000A           ; ////       - fill can be YES or NO                                   ////
 000A           ; ////       - color can be ON or OFF                                  ////
 000A           ; ////                                                                 ////
 000A           ; ////  glcd_text57(x, y, textptr, size, color)                        ////
 000A           ; ////     * Write the null terminated text pointed to by textptr with ////
 000A           ; ////       the upper left coordinate of the first character at (x,y) ////
 000A           ; ////       Characters are 5 pixels wide and 7 pixels tall            ////
 000A           ; ////       - size is an integer that scales the size of the text     ////
 000A           ; ////       - color is ON or OFF                                      ////
 000A           ; ////     * Note - This function wraps characters to the next line    ////
 000A           ; ////              use #define GLCD_WIDTH to specify a display width  ////
